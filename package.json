{
  "name": "jest-matcher-vue-test-utils",
  "version": "1.3.0",
  "description": "Jest matchers to test Vue components easily",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch -o"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hmsk/jest-matcher-vue-test-utils.git"
  },
  "keywords": [
    "vue",
    "test",
    "jest",
    "matcher"
  ],
  "author": "Kengo Hamasaki <k.hamasaki@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmsk/jest-matcher-vue-test-utils/issues"
  },
  "homepage": "https://github.com/hmsk/jest-matcher-vue-test-utils#readme",
  "devDependencies": {
    "@types/jest": "23.3.12",
    "@vue/test-utils": "1.0.0-beta.28",
    "jest": "23.6.0",
    "rollup": "1.1.2",
    "rollup-plugin-cleanup": "3.1.0",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-typescript2": "0.18.1",
    "ts-jest": "23.10.5",
    "typescript": "3.2.2",
    "vue": "2.5.21",
    "vue-jest": "3.0.2",
    "vue-template-compiler": "2.5.21"
  },
  "peerDependencies": {
    "@vue/test-utils": "^1.0.0-beta.20",
    "jest": "^22.0.0 || ^23.0.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "./test/tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "testURL": "http://localhost/",
    "preset": "ts-jest"
  }
}
